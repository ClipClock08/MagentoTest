<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Test_Feedback::test" title="Feedback" translate="title" sortOrder="100">
                    <resource id="Test_Feedback::feedback_categories" title="Feedback Categories" translate="title" sortOrder="10" />
                    <resource id="Test_Feedback::feedback_questions" title="Feedback Questions" translate="title" sortOrder="20" />
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Test_Feedback::feedback_settings" title="Settings" translate="title" sortOrder="100"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
